{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "WeatherMatrics",
					"description": "Weather matrics"
				},
				{
					"name": "DateDim",
					"description": "Dimension Date"
				},
				{
					"name": "RegionDim",
					"description": "dimension Region"
				}
			],
			"sinks": [
				{
					"name": "weathermatricss"
				},
				{
					"name": "datedimss"
				},
				{
					"name": "regionsink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          DateID as string,",
				"          RegionID as string,",
				"          AverageTemperature as string,",
				"          Humidity as string,",
				"          WindSpeed as string,",
				"          Rainfall as string,",
				"          SunshineHours as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> WeatherMatrics",
				"source(output(",
				"          DateID as string,",
				"          Date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> DateDim",
				"source(output(",
				"          RegionID as string,",
				"          RegionName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> RegionDim",
				"WeatherMatrics sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'weathermatricsPre',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> weathermatricss",
				"DateDim sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'dateDimePre',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> datedimss",
				"RegionDim sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'regionDimPre',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> regionsink"
			]
		}
	}
}