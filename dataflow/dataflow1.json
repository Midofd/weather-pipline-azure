{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "datedim",
						"type": "DatasetReference"
					},
					"name": "DateDim",
					"description": "Dimension Date"
				},
				{
					"dataset": {
						"referenceName": "regiondim",
						"type": "DatasetReference"
					},
					"name": "RegionDim",
					"description": "dimension Region"
				},
				{
					"dataset": {
						"referenceName": "weatherMatrics",
						"type": "DatasetReference"
					},
					"name": "weatherMatrics"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "datedimPre",
						"type": "DatasetReference"
					},
					"name": "datedimSink"
				},
				{
					"dataset": {
						"referenceName": "regiondimPre",
						"type": "DatasetReference"
					},
					"name": "regionSink"
				},
				{
					"dataset": {
						"referenceName": "weathermatricsPre",
						"type": "DatasetReference"
					},
					"name": "weathermatricsSink"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "SurrogateKey"
				}
			],
			"scriptLines": [
				"source(output(",
				"          DateID as integer,",
				"          Date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DateDim",
				"source(output(",
				"          RegionID as integer,",
				"          RegionName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RegionDim",
				"source(output(",
				"          DateID as integer,",
				"          RegionID as integer,",
				"          AverageTemperature as integer,",
				"          Humidity as integer,",
				"          WindSpeed as integer,",
				"          Rainfall as integer,",
				"          SunshineHours as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> weatherMatrics",
				"DateDim derive(Year = year(Date),",
				"          Month = month(Date),",
				"          Day = dayOfMonth(Date)) ~> derivedColumn1",
				"weatherMatrics keyGenerate(output(ID as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SurrogateKey",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'dateDimePre',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> datedimSink",
				"RegionDim sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DateID as string,",
				"          Date as string",
				"     ),",
				"     filePattern:'regionDimPre',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> regionSink",
				"SurrogateKey sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DateID as string,",
				"          Date as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> weathermatricsSink"
			]
		}
	}
}